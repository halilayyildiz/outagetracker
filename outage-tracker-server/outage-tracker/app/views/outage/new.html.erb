<h1>New Outage</h1>

<%= form_for @outage, url: outage_path do |f| %>
    <p>
      <%= f.label :outage_id %><br>
      <%= f.text_field :outage_id %>
    </p>

    <p>
      <%= f.label :description %><br>
      <%= f.text_area :description %>
    </p>

    <p>
      <%= f.submit %>
    </p>
<% end %>
